<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="89.5dp"
        android:gravity="center"
        android:paddingTop="50px"
        android:paddingBottom="150px"
        android:background="@android:color/transparent"
        android:id="@+id/linearLayout1"
        android:layout_marginBottom="0.0dp"
        android:minWidth="25px"
        android:minHeight="25px">
    </LinearLayout>
    <ImageView
        android:src="@android:drawable/ic_menu_gallery"
        android:layout_width="match_parent"
        android:layout_height="338.5dp"
        android:layout_marginStart="90px"
        android:layout_marginEnd="90px"
        android:layout_marginBottom="0.0dp"
        android:id="@+id/imageView1" />
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:paddingTop="50px"
        android:paddingBottom="50px"
        android:background="@android:color/transparent"
        android:id="@+id/linearLayout2"
        android:layout_marginBottom="0.0dp"
        android:layout_marginTop="0.0dp"
        android:minWidth="25px"
        android:minHeight="25px">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="227.6dp"
            android:gravity="center"
            android:paddingTop="50px"
            android:paddingBottom="50px"
            android:background="@android:color/transparent"
            android:id="@+id/linearLayout2"
            android:layout_marginBottom="64.7dp"
            android:layout_marginTop="0.0dp">
            <Button
                android:text="@string/log_in"
                android:textSize="20dp"
                android:layout_marginBottom="5dp"
                android:layout_width="260.5dp"
                android:layout_height="55.5dp"
                android:backgroundTint="#248fef"
                android:textColor="#ffffff"
                android:background="@drawable/topcorners"
                android:id="@+id/button1" />
            <Button
                android:text="@string/sign_up"
                android:textSize="20dp"
                android:layout_width="260.5dp"
                android:layout_height="55.5dp"
                android:backgroundTint="#1f6fb6"
                android:textColor="#ffffff"
                android:background="@drawable/bottomcorners"
                android:id="@+id/button2" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
